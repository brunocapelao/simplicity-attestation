// Admin Public Key: bcc13efe121b35c7270c41ce63dab7688c0326c58c5f5e1d1af317c1503cad45

fn main() {
    let sig: Signature = witness("ADMIN_SIGNATURE");
    let msg: u256 = jet::sig_all_hash();

    // Admin public key (from TESTNET_KEYS.md)
    let pk: Pubkey = 0xbcc13efe121b35c7270c41ce63dab7688c0326c58c5f5e1d1af317c1503cad45;

    jet::bip_0340_verify((pk, msg), sig);
}
